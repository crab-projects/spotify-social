  "bugs": {
    "url": "https://github.com/crab-projects/spotify-social/issues"
  },
  "homepage": "https://github.com/crab-projects/spotify-social#readme",
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "axios": "^0.21.1",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "find-config": "^1.0.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.11",
